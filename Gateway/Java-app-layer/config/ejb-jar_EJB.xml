<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar version="2.1" xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/ejb-jar_2_1.xsd">
    <enterprise-beans>
        <session>
            <display-name>EJB Gateway</display-name>
            <ejb-name>GW_EJB_Bean</ejb-name>
            <home>com.iflex.fcubs.gw.ejb.GWEJBRemoteHome</home>
            <remote>com.iflex.fcubs.gw.ejb.GWEJBRemote</remote>
            <local-home>com.iflex.fcubs.gw.ejb.GWEJBLocalHome</local-home>
            <local>com.iflex.fcubs.gw.ejb.GWEJBLocal</local>
            <ejb-class>com.iflex.fcubs.gw.ejb.GWEJBBean</ejb-class>
            <session-type>Stateless</session-type>
            <transaction-type>Container</transaction-type>
			<!-- KERNEL10.0 Property file Path Change STARTS -->
			<env-entry>       
				<env-entry-name>propertyPath</env-entry-name>       
				<env-entry-type>java.lang.String</env-entry-type>
				<env-entry-value>D:/Iflex/FCJ/Kernel/</env-entry-value>
			</env-entry>
			<!-- KERNEL10.0 Property file Path Change ENDS -->
            <resource-ref> 
                <res-ref-name>FLEXTEST.WORLD</res-ref-name> 
                <res-type>javax.sql.XADataSource</res-type> 
                <res-auth>Container</res-auth>
            </resource-ref>
            
        </session>
    </enterprise-beans>
    <assembly-descriptor>
        <container-transaction>
            <method>
                <ejb-name>GW_EJB_Bean</ejb-name>
                <method-name>*</method-name>
            </method>
            <trans-attribute>RequiresNew</trans-attribute>
        </container-transaction>
    </assembly-descriptor>
</ejb-jar>

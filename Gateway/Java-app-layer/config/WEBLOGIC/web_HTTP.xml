<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app id="WebApp_ID">
    <display-name>HTTP Servlet Gateway</display-name>
    <servlet id="Servlet_1">
        <servlet-name>GWHttpServlet</servlet-name>
        <servlet-class>com.iflex.fcubs.gw.http.servlets.GWHttpServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet-mapping id="ServletMapping_1">
        <servlet-name>GWHttpServlet</servlet-name>
        <url-pattern>/GWHttpServlet</url-pattern>
    </servlet-mapping>

	<!-- KERNEL10.0 Property file Path Change STARTS -->
	<env-entry>
			 <description>Property File Path</description>
			 <env-entry-name>propertyPath</env-entry-name>			 
			 <env-entry-type>java.lang.String</env-entry-type> 
			 <env-entry-value>D:/Kernel10.2/GW_HTTP/config/</env-entry-value>
    </env-entry>
	<!-- KERNEL10.0 Property file Path Change ENDS -->
	

	<!-- 
		KERNEL10.2 Release as Weblogic does not support the ejb look up by ejb reference name if EJB module is not part of Servlet Application 
	-->
	<!-- EJB Remote Reference Entry STARTS-->
	<ejb-ref>
		<ejb-ref-name>GW_EJB_Bean</ejb-ref-name>
		<ejb-ref-type>Session</ejb-ref-type>
		<home>com.iflex.fcubs.gw.ejb.GWEJBRemoteHome</home>
		<remote>com.iflex.fcubs.gw.ejb.GWEJBRemote</remote>
	</ejb-ref>
	<!-- EJB Remote Reference Entry ENDS-->
	
	
</web-app>

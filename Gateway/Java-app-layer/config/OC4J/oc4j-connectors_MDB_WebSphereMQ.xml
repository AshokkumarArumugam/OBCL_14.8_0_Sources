<?xml version="1.0"?>
<!DOCTYPE oc4j-connectors PUBLIC "-//Oracle//DTD Connectors configuration" "http://xmlns.oracle.com/ias/dtds/oc4j-connectors.dtd">

<oc4j-connectors>
    <connector name="WebSphereMQC" path="WebSphereMQ.rar">

        <adminobject-config location="MDB_QUEUE_RESPONSE">
            <adminobject-class>oracle.j2ee.ra.jms.generic.AdminObjectQueueImpl</adminobject-class>
            <config-property name="jndiName" value="MDB_QUEUE_RESPONSE"/>
            <config-property name="resourceProviderName" value="WebSphereMQRP"/>
        </adminobject-config>

        <adminobject-config location="MDB_QUEUE">
            <adminobject-class>oracle.j2ee.ra.jms.generic.AdminObjectQueueImpl</adminobject-class>
            <config-property name="jndiName" value="MDB_QUEUE"/>
            <config-property name="resourceProviderName" value="WebSphereMQRP"/>
        </adminobject-config>

        <adminobject-config location="MDB_QUEUE_DLQ">
            <adminobject-class>oracle.j2ee.ra.jms.generic.AdminObjectQueueImpl</adminobject-class>
            <config-property name="jndiName" value="MDB_QUEUE_DLQ"/>
            <config-property name="resourceProviderName" value="WebSphereMQRP"/>
        </adminobject-config>

    </connector>
</oc4j-connectors>

<?xml version = '1.0' encoding = 'windows-1252'?>
<weblogic-ejb-jar xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://www.bea.com/ns/weblogic/weblogic-ejb-jar.xsd"
                  xmlns="http://www.bea.com/ns/weblogic/weblogic-ejb-jar">
<weblogic-enterprise-bean>
<ejb-name>COEJBBean</ejb-name>
<jndi-name>{REF_LOCATION}/ejb/COEJBBean#com.ofss.core.co.ejb.ICOEJB</jndi-name>
<local-jndi-name>{REF_LOCATION}/ejb/local/COEJBBean#com.ofss.core.co.ejb.ICOEJB</local-jndi-name>
<enable-call-by-reference>True</enable-call-by-reference>
</weblogic-enterprise-bean>
</weblogic-ejb-jar>
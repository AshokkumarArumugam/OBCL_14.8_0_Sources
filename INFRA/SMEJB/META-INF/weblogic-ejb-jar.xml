<?xml version = '1.0' encoding = 'windows-1252'?>
<weblogic-ejb-jar xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://www.bea.com/ns/weblogic/weblogic-ejb-jar.xsd"
                  xmlns="http://www.bea.com/ns/weblogic/weblogic-ejb-jar">
<weblogic-enterprise-bean>
<ejb-name>SMEJBBean</ejb-name>
<jndi-name>{REF_LOCATION}/ejb/SMEJBBean#com.ofss.sms.sm.ejb.ISMEJB</jndi-name>
<local-jndi-name>{REF_LOCATION}/ejb/local/SMEJBBean#com.ofss.sms.sm.ejb.ISMEJB</local-jndi-name>
<enable-call-by-reference>True</enable-call-by-reference>
</weblogic-enterprise-bean>
<weblogic-enterprise-bean>
<ejb-name>SMSchedulerEJBBean</ejb-name>
<jndi-name>{REF_LOCATION}/ejb/SMSchedulerEJBBean#com.ofss.sms.sm.ejb.ISMSchedulerEJB</jndi-name>
<local-jndi-name>{REF_LOCATION}/ejb/local/SMSchedulerEJBBean#com.ofss.sms.sm.ejb.ISMEJBSchedulerEJB</local-jndi-name>
<enable-call-by-reference>True</enable-call-by-reference>
</weblogic-enterprise-bean>

</weblogic-ejb-jar>
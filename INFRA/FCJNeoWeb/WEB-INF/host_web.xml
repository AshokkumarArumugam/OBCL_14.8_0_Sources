<host_web>
	<listener>
		<listener-class>com.ofss.fcc.common.FCUserSessions</listener-class>
	</listener>
	<filter-mapping>
		<filter-name>AuthenticationFilter</filter-name>
		<servlet-name>FCCommonServlet</servlet-name>
	</filter-mapping>
	<!-- Ullas: WAS8.5 compatibility :Corrected the case sensitive servlet name -->
	<filter-mapping>
		<filter-name>AuthenticationFilter</filter-name>
		<servlet-name>FileUploadServlet</servlet-name>
	</filter-mapping>
	<!--<filter-mapping> Commented for 18629265  start
		<filter-name>AuthenticationFilter</filter-name>
		<servlet-name>UnTankServlet</servlet-name>
	</filter-mapping> commented for 18629265  end-->
	<servlet>
		<servlet-name>HostUploadServlet</servlet-name>
		<servlet-class>com.ofss.fcc.web.HostUploadServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>PingServlet</servlet-name>
		<servlet-class>com.ofss.host.oindicator.PingServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>FCCommonServlet</servlet-name>
		<servlet-class>com.ofss.fcc.web.FCCommonServlet</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet>
		<servlet-name>FileUploadServlet</servlet-name>
		<servlet-class>com.ofss.fcc.web.FileUploadServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>HostUploadServlet</servlet-name>
		<url-pattern>/HostUploadServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>PingServlet</servlet-name>
		<url-pattern>/PingServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>FCCommonServlet</servlet-name>
		<url-pattern>/FCCommonServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>FileUploadServlet</servlet-name>
		<url-pattern>/fileuploadservlet</url-pattern>
	</servlet-mapping>

<pm_web>

	<filter-mapping>
		<filter-name>AuthenticationFilter</filter-name>
		<servlet-name>FileEnvelopServlet</servlet-name>
	</filter-mapping>
	<servlet>
		<servlet-name>FileEnvelopServlet</servlet-name>
		<servlet-class>com.ofss.fcc.web.FileEnvelopServlet</servlet-class>
		<multipart-config>
			<max-file-size>1048576</max-file-size>
		</multipart-config>
	</servlet>
	<servlet-mapping>
		<servlet-name>FileEnvelopServlet</servlet-name>
		<url-pattern>/fileenvelopservlet</url-pattern>
	</servlet-mapping>
	
</pm_web>

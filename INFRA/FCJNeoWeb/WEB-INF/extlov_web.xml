<ext_lov>
	<servlet-mapping> 
	      <servlet-name>External Lov Rest Service</servlet-name> 
	      <url-pattern>/LovService/*</url-pattern> 
    </servlet-mapping>   
    <servlet> 
          <servlet-name>External Lov Rest Service</servlet-name> 
          <servlet-class>org.glassfish.jersey.servlet.ServletContainer</servlet-class> 
	      <init-param> 
	         <param-name>javax.ws.rs.Application</param-name> 
	         <param-value>com.ofss.fcc.handlers.FCExtLovRSApplication</param-value> 
	      </init-param> 
	      <load-on-startup>3</load-on-startup>
	</servlet>
</ext_lov>
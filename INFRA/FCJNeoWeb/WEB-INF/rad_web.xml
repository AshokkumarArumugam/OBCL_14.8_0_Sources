<rad_web>
        <filter>
		<filter-name>AuthenticationFilter</filter-name>
		<filter-class>com.ofss.infra.web.AuthenticationFilter</filter-class>
</filter>
<filter-mapping>
		<filter-name>AuthenticationFilter</filter-name>
		<servlet-name>UIControllerServlet</servlet-name>
</filter-mapping>
<filter-mapping>
		<filter-name>AuthenticationFilter</filter-name>
		<servlet-name>CodeGeneratorServlet</servlet-name>
</filter-mapping>
<filter-mapping>
		<filter-name>AuthenticationFilter</filter-name>
		<servlet-name>SchemaManagerServlet</servlet-name>
</filter-mapping>
<filter-mapping>
		<filter-name>AuthenticationFilter</filter-name>
		<servlet-name>ScriptsGeneratorServlet</servlet-name>
</filter-mapping>
  <servlet>
    <servlet-name>UIControllerServlet</servlet-name>
    <servlet-class>com.ofss.tool.web.UIControllerServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>CodeGeneratorServlet</servlet-name>
    <servlet-class>com.ofss.tool.web.CodeGeneratorServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>SchemaManagerServlet</servlet-name>
    <servlet-class>com.ofss.tool.web.SchemaManagerServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ScriptsGeneratorServlet</servlet-name>
    <servlet-class>com.ofss.tool.web.ScriptsGeneratorServlet</servlet-class>
  </servlet>
	 <servlet-mapping>
    <servlet-name>UIControllerServlet</servlet-name>
    <url-pattern>/UIControllerServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CodeGeneratorServlet</servlet-name>
    <url-pattern>/CodeGeneratorServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SchemaManagerServlet</servlet-name>
    <url-pattern>/SchemaManagerServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ScriptsGeneratorServlet</servlet-name>
    <url-pattern>/ScriptsGeneratorServlet</url-pattern>
  </servlet-mapping>

</rad_web>

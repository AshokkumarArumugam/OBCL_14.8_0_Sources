<?xml version="1.0" encoding="UTF-8"?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd"
             version="2.0">

    <persistence-unit name="{REF_LOCATION}/COEntities" transaction-type="JTA">
        <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
        <jta-data-source>{INIT_DS}</jta-data-source>
         <jar-file>COEJBEJB.jar</jar-file>
         <jar-file>PIEJBEJB.jar</jar-file>
        <properties>
            <!--<property name="eclipselink.composite-unit" value="true"/>-->
            <property name="eclipselink.target-server" value="{TARGET}"/>
            <property name="eclipselink.logging.level" value="FINEST"/>
            <property name="javax.persistence.jtaDataSource" value="{INIT_DS}"/>
        </properties>
    </persistence-unit>
</persistence>
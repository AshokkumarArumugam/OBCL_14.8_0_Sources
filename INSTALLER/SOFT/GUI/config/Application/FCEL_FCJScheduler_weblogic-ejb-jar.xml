<weblogic-ejb-jar>
  <weblogic-enterprise-bean>
    <ejb-name>SchedulerEJB</ejb-name>
    <jndi-name>{REF_LOCATION}/ejb/SchedulerEJBHome</jndi-name>
    <local-jndi-name>{REF_LOCATION}/ejb/local/SchedulerEJBHome</local-jndi-name>
  </weblogic-enterprise-bean>
  <weblogic-enterprise-bean>
    <ejb-name>GWNotifyMDBBean</ejb-name>
    <message-driven-descriptor>
      <pool>
        <max-beans-in-free-pool>5</max-beans-in-free-pool>
        <initial-beans-in-free-pool>5</initial-beans-in-free-pool>
      </pool>
      <destination-jndi-name>{NOTIFY_QUEUE}</destination-jndi-name>
      <connection-factory-jndi-name>{NOTIFY_MDB_QCF}</connection-factory-jndi-name>
    
      <jms-polling-interval-seconds>20</jms-polling-interval-seconds>
    </message-driven-descriptor>
  </weblogic-enterprise-bean>
  <weblogic-enterprise-bean>
    <ejb-name>EMSOutMDB</ejb-name>
    <message-driven-descriptor>
      <pool>
        <max-beans-in-free-pool>5</max-beans-in-free-pool>
        <initial-beans-in-free-pool>5</initial-beans-in-free-pool>
      </pool>
      <destination-jndi-name>EMS_OUTQUEUE</destination-jndi-name>
      <connection-factory-jndi-name>{EMS_OUTQCF}</connection-factory-jndi-name>
     
      <jms-polling-interval-seconds>20</jms-polling-interval-seconds>
    </message-driven-descriptor>
    <transaction-descriptor>
      <trans-timeout-seconds>3600</trans-timeout-seconds>
    </transaction-descriptor>
  </weblogic-enterprise-bean>
  <weblogic-enterprise-bean>
    <ejb-name>EMSInMDB</ejb-name>
    <message-driven-descriptor>
      <pool>
        <max-beans-in-free-pool>5</max-beans-in-free-pool>
        <initial-beans-in-free-pool>5</initial-beans-in-free-pool>
      </pool>
      <destination-jndi-name>EMS_INQUEUE</destination-jndi-name>
      <!--
	  <initial-context-factory>weblogic.jndi.WLInitialContextFactory</initial-context-factory>
      <provider-url>t3://127.0.0.1:8003</provider-url>
      -->
      <connection-factory-jndi-name>{EMS_INQCF}</connection-factory-jndi-name>
      <jms-polling-interval-seconds>20</jms-polling-interval-seconds>
    </message-driven-descriptor>
    <transaction-descriptor>
      <trans-timeout-seconds>3600</trans-timeout-seconds>
    </transaction-descriptor>
  </weblogic-enterprise-bean>
  
  <weblogic-enterprise-bean>
        <ejb-name>BIPReportEJB</ejb-name>

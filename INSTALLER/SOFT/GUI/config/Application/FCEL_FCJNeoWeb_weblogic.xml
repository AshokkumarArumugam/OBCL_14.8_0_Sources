	</virtual-directory-mapping> 
	<virtual-directory-mapping> 
	<local-path>{STATIC_DATA_PATH}</local-path> 
	<url-pattern>/UIXML/*</url-pattern> 
	<url-pattern>*.xml</url-pattern> 
	</virtual-directory-mapping>

	

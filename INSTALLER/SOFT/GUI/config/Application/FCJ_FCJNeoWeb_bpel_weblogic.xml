<?xml version="1.0" encoding="UTF-8"?>
<wls:weblogic-web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:wls="http://www.bea.com/ns/weblogic/90" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd http://www.bea.com/ns/weblogic/90 http://www.bea.com/ns/weblogic/90/weblogic-web-app.xsd">
	<container-descriptor>
	<prefer-application-resources>
	   <resource-name>META-INF/services/com.sun.xml.ws.api.pipe.TubelineAssemblerFactory</resource-name>
	</prefer-application-resources>
	</container-descriptor>	
<!-- BOTG-jersey 18426143-->
	<wls:library-ref>
        <wls:library-name>jax-rs</wls:library-name>
    </wls:library-ref>	
<!-- BOTG-jersey 18426143-->
<!--Below entries are removed for for bug 19443634  -->	
	<wls:session-descriptor>
        <!--MAA-UI-02-->
        <wls:persistent-store-type>replicated_if_clustered</wls:persistent-store-type>
        <!--<wls:cookie-secure>true</wls:cookie-secure>        -->
        <wls:url-rewriting-enabled>false</wls:url-rewriting-enabled>
        <wls:cookie-http-only>true</wls:cookie-http-only>
    </wls:session-descriptor>
    <virtual-directory-mapping>
        <local-path>{STATIC_DATA_PATH}</local-path>
        <url-pattern>/Script/JS/*</url-pattern>
        <url-pattern>*.js</url-pattern>
    </virtual-directory-mapping>
    <virtual-directory-mapping>
        <local-path>{STATIC_DATA_PATH}</local-path>
        <url-pattern>/UIXML/*</url-pattern>
        <url-pattern>*.xml</url-pattern>
    </virtual-directory-mapping>   
    <context-root>{APP_CONTEXT}</context-root>
</wls:weblogic-web-app>
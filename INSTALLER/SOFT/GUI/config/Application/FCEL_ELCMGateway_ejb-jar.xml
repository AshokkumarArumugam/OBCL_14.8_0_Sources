<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/ejb-jar_3_0.xsd">
  <display-name>ELGateway</display-name>
  <enterprise-beans>  		         
    <session>
        <ejb-name>ELGWEJBBean</ejb-name>
        <mapped-name>ejb/{REF_LOCATION}/ELCMGateway.jar/ELGWEJBBean</mapped-name>
        <ejb-class>com.ofss.elcm.ejb.ELGWEJBBean</ejb-class>
        <session-type>Stateless</session-type>
        <transaction-type>Container</transaction-type>
        <env-entry>
          <env-entry-name>RES_APP_NAME</env-entry-name>
          <env-entry-type>java.lang.String</env-entry-type>
          <env-entry-value>{REF_LOCATION}</env-entry-value>
        </env-entry>
    </session> 
         <!--Added starts-->
    <session>     
        <ejb-name>ELSchedulerEJBBean</ejb-name>
        <mapped-name>ejb/{REF_LOCATION}/ELCMGateway.jar/ELSchedulerEJBBean</mapped-name>
        <ejb-class>com.ofss.elcm.ejb.ELSchedulerEJBBean</ejb-class>
        <session-type>Stateless</session-type>
        <transaction-type>Container</transaction-type>
        <env-entry>
            <env-entry-name>RES_APP_NAME</env-entry-name>
            <env-entry-type>java.lang.String</env-entry-type>
            <env-entry-value>{REF_LOCATION}</env-entry-value>
        </env-entry>
		<env-entry>
          <env-entry-name>LOG_PATH</env-entry-name>
          <env-entry-type>java.lang.String</env-entry-type>
          <env-entry-value>{REF_WORK_AREA}</env-entry-value>
        </env-entry>
        <persistence-context-ref>
                <persistence-context-ref-name>{REF_LOCATION}/ELCMEntities</persistence-context-ref-name>
                <persistence-unit-name>{REF_LOCATION}/ELCMEntities</persistence-unit-name>
        </persistence-context-ref>
        <persistence-context-ref>
                <persistence-context-ref-name>{REF_LOCATION}/ELCMRepEntities</persistence-context-ref-name>
                <persistence-unit-name>{REF_LOCATION}/ELCMRepEntities</persistence-unit-name>
        </persistence-context-ref>
    </session>          
    <message-driven>
        <display-name>Gateway MDBBean</display-name>
        <ejb-name>ELGWMDB</ejb-name>
        <ejb-class>com.ofss.elcm.mdb.ELGWMDB</ejb-class>
        <messaging-type>javax.jms.MessageListener</messaging-type>
        <transaction-type>Container</transaction-type>
        <env-entry>
            <env-entry-name>RES_APP_NAME</env-entry-name>
            <env-entry-type>java.lang.String</env-entry-type>
            <env-entry-value>{REF_LOCATION}</env-entry-value>
        </env-entry>
    </message-driven> 
    <message-driven>
        <display-name>Notification MDBBean</display-name>
        <ejb-name>ELNotifyMDB</ejb-name>
        <ejb-class>com.ofss.elcm.mdb.ELNotifyMDB</ejb-class>
        <messaging-type>javax.jms.MessageListener</messaging-type>
        <transaction-type>Container</transaction-type>
        <env-entry>
            <env-entry-name>RES_APP_NAME</env-entry-name>
            <env-entry-type>java.lang.String</env-entry-type>
            <env-entry-value>{REF_LOCATION}</env-entry-value>
        </env-entry>
        <persistence-context-ref>
                <persistence-context-ref-name>{REF_LOCATION}/ELCMEntities</persistence-context-ref-name>
                <persistence-unit-name>{REF_LOCATION}/ELCMEntities</persistence-unit-name>
        </persistence-context-ref>
        <persistence-context-ref>
                <persistence-context-ref-name>{REF_LOCATION}/ELCMRepEntities</persistence-context-ref-name>
                <persistence-unit-name>{REF_LOCATION}/ELCMRepEntities</persistence-unit-name>
        </persistence-context-ref>
    </message-driven> 
    <!--Added ends-->
    </enterprise-beans>
</ejb-jar>
				
				

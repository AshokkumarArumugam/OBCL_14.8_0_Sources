<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/ejb-jar_3_0.xsd">
  <display-name>ELGateway</display-name>
  <enterprise-beans>  		
            <session>
                    <ejb-name>IReplicationEJBBean</ejb-name>
                    <mapped-name>ejb/{REF_LOCATION}/ELCMEJB.jar/IReplicationEJBBean</mapped-name>
                    <ejb-class>com.ofss.elcm.replicate.ejb.ReplicationEJBBean</ejb-class>
                    <session-type>Stateless</session-type>
                    <transaction-type>Bean</transaction-type>
            </session>
            <session>
                    <ejb-name>IReplSchedulerEJB</ejb-name>
                    <mapped-name>ejb/{REF_LOCATION}/ELCMEJB.jar/IReplSchedulerEJB</mapped-name>
                    <ejb-class>com.ofss.elcm.replicate.ejb.ReplSchedulerEJB</ejb-class>
                    <session-type>Stateless</session-type>
                    <transaction-type>Bean</transaction-type>
            </session>
            <session>
                    <ejb-name>ISchedulerEJB</ejb-name>
                    <mapped-name>ejb/{REF_LOCATION}/ELCMEJB.jar/ISchedulerEJB</mapped-name>
                    <ejb-class>com.ofss.elcm.scheduler.ELSchedulerEJBBean</ejb-class>
                    <session-type>Stateless</session-type>
                    <transaction-type>Bean</transaction-type>
            </session>
            <session>
                    <ejb-name>IGWEJB</ejb-name>
                    <mapped-name>ejb/{REF_LOCATION}/ELGateway.jar/IGWEJB</mapped-name>
                    <ejb-class>com.ofss.elcm.gtw.ejb.GWEJBBean</ejb-class>
                    <session-type>Stateless</session-type>
                    <transaction-type>Bean</transaction-type>
                            <env-entry>
                                                    <env-entry-name>RES_APP_NAME</env-entry-name>
                                                    <env-entry-type>java.lang.String</env-entry-type>
                                                    <env-entry-value>{REF_LOCATION}</env-entry-value>
                            </env-entry>
            </session>
            <message-driven>
                    <display-name>NOTIFY MDB Bean</display-name>
                    <ejb-name>ELNotifyMDBBean</ejb-name>
                    <ejb-class>com.ofss.elcm.scheduler.ELNotifyMDBBean</ejb-class>
                    <messaging-type>javax.jms.MessageListener</messaging-type>
                    <transaction-type>Container</transaction-type>
                            <env-entry>
                            <env-entry-name>RES_APP_NAME</env-entry-name>
                            <env-entry-type>java.lang.String</env-entry-type>
                            <env-entry-value>{REF_LOCATION}</env-entry-value>
                            </env-entry>
            </message-driven>
            </enterprise-beans>
 </ejb-jar>
<weblogic-ejb-jar>
  <weblogic-enterprise-bean>
    <ejb-name>RecordingMDBBean</ejb-name>
    <message-driven-descriptor>
      <pool>
        <max-beans-in-free-pool>5</max-beans-in-free-pool>
        <initial-beans-in-free-pool>5</initial-beans-in-free-pool>
      </pool>
      <destination-jndi-name>{RTT_RECORDING_QUEUE}</destination-jndi-name>
      <!--<initial-context-factory>weblogic.jndi.WLInitialContextFactory</initial-context-factory>
      <provider-url>t3://localhost:7101</provider-url> -->
      <connection-factory-jndi-name>{RTTDestQCF}</connection-factory-jndi-name>     
      <jms-polling-interval-seconds>20</jms-polling-interval-seconds>
    </message-driven-descriptor>
    <transaction-descriptor>
      <trans-timeout-seconds>3600</trans-timeout-seconds>
    </transaction-descriptor>
  </weblogic-enterprise-bean>  
  <weblogic-enterprise-bean>
    <ejb-name>OFATSReplayEJB</ejb-name>
    <jndi-name>{REF_LOCATION}/ejb/OFATSREPLAYEJBHome</jndi-name>
    <local-jndi-name>ejb/local/OFATSREPLAYEJBHome</local-jndi-name>
  </weblogic-enterprise-bean>  
</weblogic-ejb-jar>
</FCJRTT>

<deployment-plan global-variables="false" xsi:schemaLocation="http://xmlns.oracle.com/weblogic/deployment-plan http://xmlns.oracle.com/weblogic/deployment-plan/1.0/deployment-plan.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.oracle.com/weblogic/deployment-plan">

<application-name>{APPLICATION_NAME}</application-name>


<variable-definition>


<variable>

<name>PROVIDER_URL</name>

<value>{PROVIDER_URL}</value>

</variable>

<variable>

<name>EMS_PROVIDER_URL</name>

<value>{EMS_PROVIDER_URL}</value>

</variable>

<variable>
<name>role_name</name>

<value>{ROLE_NAME}</value>

</variable>

<variable>

		<RAD_LOVS ID="LOV_ACCOUNTS">
			<LOV_NAME>LOV_ACCOUNTS</LOV_NAME>
			<LOV_QUERY>SELECT * FROM (SELECT ac_gl_no, ac_gl_ccy, IBAN_AC_NO, branch_code, AC_OR_GL, GL_CATEGORY from oltbs_account where ac_gl_rec_status = 'O' and ('Y', 'Y') = (select nvl(A.param_value, 'N'), NVL(B.param_value, 'N') from oltbs_loan_param_detail a, oltbs_loan_param_detail b where a.param_name = 'ALLOW_ALL_ACC_IN_OSI' and b.param_name = 'ALLOW_ALL_GL_ACC_IN_OSI') union (select ac_gl_no, ac_gl_ccy, IBAN_AC_NO, branch_code, AC_OR_GL, GL_CATEGORY FROM oltbs_account WHERE ac_gl_rec_status = 'O' AND AC_OR_GL = 'G' and ('Y', 'Y') not in (select nvl(A.param_value, 'N'), nvl(B.param_value, 'N') from oltbs_loan_param_detail A, oltbs_loan_param_detail B where a.param_name = 'ALLOW_ALL_ACC_IN_OSI' and b.param_name = 'ALLOW_ALL_GL_ACC_IN_OSI'))) A WHERE EXISTS (SELECT * FROM olvws_PROD_ACNT_Access X WHERE A.AC_GL_NO = X.AC_GL_NO AND X.PRODUCT_CODE = ?) order by ac_gl_no</LOV_QUERY>
			<RELEASE_NAME>OFCL_12.4.0.0.0</RELEASE_NAME>

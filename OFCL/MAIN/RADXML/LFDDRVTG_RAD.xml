<RAD_LOVS ID="LOV_AMOUNT_TAG">
<LOV_NAME>LOV_AMOUNT_TAG</LOV_NAME>
<LOV_QUERY>SELECT AMOUNT_TAG, DESCRIPTION, 'N' YN_DERIVED, decode((select count(1) from lftm_interest_class a where BASIS_AMOUNT_CATEGORY = 'D' and AMOUNT_TAG like CLASS_CODE||'%'), 0, 'N', 'Y') YN_PENALTY FROM OLTBS_AMOUNT_TAG WHERE MODULE = 'OL' UNION SELECT DERIVED_TAG, TAG_DESC, 'Y' YN_DERIVED, decode((select count(1) from lftm_interest_class a where BASIS_AMOUNT_CATEGORY = 'D' and CLASS_CODE in (select substr(ACTUAL_TAG, 1, length(ACTUAL_TAG) - 4) from OLTBS_DERIVED_TAG a where b.DERIVED_TAG = a.DERIVED_TAG)), 0, 'N', 'Y') YN_PENALTY FROM OLTBS_DERIVED_TAG_MASTER b WHERE MODULE = 'OL' ORDER BY AMOUNT_TAG</LOV_QUERY>

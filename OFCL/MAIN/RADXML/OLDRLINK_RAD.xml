			<LOV_NAME>LOV_NEWLD</LOV_NAME>
			<LOV_QUERY>SELECT a.contract_ref_no Contract_ref_no, (select field_val from cstms_udf_vals where field_name = 'RAPID_ID' and rec_key = a.counterparty || '~' || substr(a.credit_line, 1, 9) || '~' || substr(a.credit_line, 10, 1) || '~') RAPID_ID, a.counterparty Counterparty, a.value_date Value_Date, a.currency Currency, a.maturity_date Maturity_Date, a.amount Amount FROM OLTBS_CONTRACT_MASTER a, oltbs_contract_preference b, oltbs_contract c, LPTBS_CONTRACT_MASTER d WHERE a.contract_ref_no = b.contract_ref_no AND a.version_no = b.version_no AND a.version_no = c.latest_version_no AND a.contract_ref_no = c.contract_ref_no AND c.contract_status IN ('A', 'Y') AND c.auth_status = 'A' AND a.agency_contract = 'Y' AND nvl(b.revolving_commitment, 'N') = (select revolving_commitment from oltbs_contract_preference v where v.contract_ref_no = ? and v.version_no = (select max(z.version_no) from oltbs_contract_preference z where z.contract_ref_no = v.contract_ref_no)) AND a.maturity_date = (SELECT maturity_date FROM lptbs_contract_master a WHERE contract_ref_no = ? AND version_no = (SELECT max(version_no) FROM lptbs_contract_master WHERE contract_ref_no = a.contract_ref_no)) AND a.currency = (SELECT currency FROM LPTBS_CONTRACT_MASTER a WHERE contract_ref_no = ? AND version_no = (SELECT max(version_no) FROM LPTBS_CONTRACT_MASTER WHERE contract_ref_no = ?)) AND not exists (SELECT 1 FROM oltbs_stp_job_browser WHERE ld_ref_no = a.contract_Ref_no) AND a.module = 'OL' and c.product_type = 'C' UNION ALL SELECT a.contract_ref_no Contract_ref_no, NULL RAPID_ID, a.counterparty Counterparty, a.value_date Value_Date, a.currency Currency, a.maturity_date Maturity_Date, a.amount Amount FROM OLTBS_CONTRACT_MASTER a, oltbs_contract_preference b, oltbs_contract c, LPTBS_CONTRACT_MASTER d WHERE a.contract_ref_no = b.contract_ref_no AND a.version_no = b.version_no AND a.version_no = c.latest_version_no AND a.contract_ref_no = c.contract_ref_no AND c.contract_status IN ('A', 'Y') AND c.auth_status = 'A' AND a.agency_contract = 'Y' AND a.maturity_date = (SELECT maturity_date FROM lptbs_contract_master a WHERE contract_ref_no = ? AND version_no = (SELECT max(version_no) FROM lptbs_contract_master WHERE contract_ref_no = a.contract_ref_no)) AND a.currency = (SELECT currency FROM LPTBS_CONTRACT_MASTER a WHERE contract_ref_no = ? AND version_no = (SELECT max(version_no) FROM LPTBS_CONTRACT_MASTER WHERE contract_ref_no =a.contract_ref_no)) AND not exists (SELECT 1 FROM OLTBS_STP_JOB_BROWSER WHERE ld_ref_no = a.contract_Ref_no) AND a.module = 'OL' AND c.product_type = 'L'</LOV_QUERY>

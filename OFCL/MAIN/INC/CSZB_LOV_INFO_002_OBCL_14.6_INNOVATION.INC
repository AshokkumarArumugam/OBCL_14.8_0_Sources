 VALUES 
('OLDREPRC',
'LOV_CONTRACT_CHILD',
'CONTRACT_REF_NO~USER_REF_NO~CONTRACT_CCY~PRINCIPAL_OUTSTANDING_BAL~MATURITY_DATE~VALUE_DATE~',
'LBL_CONTREFNO~LBL_USERREFNO~LBL_CURRENCY~LBL_OUTPRIN~LBL_MATURITYDATE~LBL_VALUEDATE~',
'CONTRACT_REF_NO~USER_REF_NO~CONTRACT_CCY~PRINCIPAL_OUTSTANDING_BAL~MATURITY_DATE~VALUE_DATE~',
'SELECT A.CONTRACT_REF_NO, A.USER_REF_NO, A.CONTRACT_CCY, B.PRINCIPAL_OUTSTANDING_BAL, D.MATURITY_DATE, D.VALUE_DATE FROM OLTBS_CONTRACT A, OLTBS_CONTRACT_BALANCE B, OLTMS_PRODUCT_MASTER_LD C, OLTBS_CONTRACT_MASTER D WHERE A.BRANCH = sypks_utils.get_branch(?) AND A.MODULE_CODE = ''OL'' AND A.CONTRACT_STATUS = ''A'' AND A.AUTH_STATUS = ''A'' AND A.CONTRACT_REF_NO = D.CONTRACT_REF_NO AND A.LATEST_VERSION_NO = D.VERSION_NO /*AND D.ROLLOVER_ALLOWED = ''N''*/ AND D.MATURITY_DATE = ? AND A.CONTRACT_REF_NO <> ? AND A.CONTRACT_REF_NO NOT IN (SELECT CHILD_REF_NO FROM OLTB_CONTRACT_MERGE_DETAIL WHERE CONTRACT_REF_NO = ? AND MERGE_SERIAL_NO = ?) AND A.COUNTERPARTY = ? AND A.CONTRACT_CCY = ? AND A.PRODUCT_TYPE = ''L'' AND B.PRINCIPAL_OUTSTANDING_BAL > 0 AND A.CONTRACT_REF_NO = B.CONTRACT_REF_NO AND A.PRODUCT_CODE = C.PRODUCT AND C.PROD_SCH_TYPE = ''N'' AND ((C.PAYMENT_METHOD = ''B'' AND 0 = (SELECT NVL(SUM(Z.AMOUNT_DUE) - SUM(Z.AMOUNT_SETTLED) + SUM(Z.ADJUSTED_AMOUNT), 0) FROM OLTBS_AMOUNT_DUE_CS Z WHERE Z.CONTRACT_REF_NO = A.CONTRACT_REF_NO AND Z.INFLOW_OUTFLOW = CASE WHEN Z.COMPONENT = ''PRINCIPAL'' THEN ''I'' ELSE Z.INFLOW_OUTFLOW END AND Z.COMPONENT_TYPE IN (''P'', ''I'') AND OLPKS_SERVICESY.FN_IS_RISTOURNE_COMPONENT(Z.COMPONENT,Z.CONTRACT_REF_NO) = ''N'' AND Z.COMPONENT NOT IN (SELECT COMPONENT FROM LFTBS_CONTRACT_INTEREST WHERE CONTRACT_REFERENCE_NO = Z.CONTRACT_REF_NO AND EVENT_SEQUENCE_NO = (SELECT MAX(EVENT_SEQUENCE_NO) FROM LFTBS_CONTRACT_INTEREST WHERE CONTRACT_REFERENCE_NO = Z.CONTRACT_REF_NO) AND NVL(CONSIDER_FOR_LIQ, ''Y'') = ''N'') AND Z.DUE_DATE < ?)) OR (C.PAYMENT_METHOD IN (''D'', ''T'') AND 0 = (SELECT NVL(SUM(Z.AMOUNT_DUE) - SUM(Z.AMOUNT_SETTLED) + SUM(Z.ADJUSTED_AMOUNT), 0) FROM OLTBS_AMOUNT_DUE_CS Z WHERE Z.CONTRACT_REF_NO = A.CONTRACT_REF_NO AND Z.INFLOW_OUTFLOW = CASE WHEN Z.COMPONENT = ''PRINCIPAL'' THEN ''I'' ELSE Z.INFLOW_OUTFLOW END AND Z.COMPONENT_TYPE IN (''P'', ''I'') AND OLPKS_SERVICESY.FN_IS_RISTOURNE_COMPONENT(Z.COMPONENT,Z.CONTRACT_REF_NO) = '||
'''N'' AND Z.COMPONENT NOT IN (SELECT COMPONENT FROM LFTBS_CONTRACT_INTEREST WHERE CONTRACT_REFERENCE_NO = Z.CONTRACT_REF_NO AND EVENT_SEQUENCE_NO = (SELECT MAX(EVENT_SEQUENCE_NO) FROM LFTBS_CONTRACT_INTEREST WHERE CONTRACT_REFERENCE_NO = Z.CONTRACT_REF_NO)) AND Z.DUE_DATE < ?))) AND NOT EXISTS (SELECT 1 FROM OLTBS_CONTRACT_SPLIT_MASTER M WHERE M.CONTRACT_REF_NO = A.CONTRACT_REF_NO AND M.AUTH_STAT = ''A'' AND M.SPLIT_STATUS = ''U'') AND A.USER_DEFINED_STATUS = (SELECT K.STATUS FROM OLTMS_PRODUCT_STATUS_MASTER K WHERE K.PRODUCT = A.PRODUCT_CODE AND K.STATUS_SEQUENCE = (SELECT L.STATUS_SEQUENCE FROM OLTMS_PRODUCT_STATUS_MASTER L WHERE L.PRODUCT = sypks_utils.get_product(?) AND L.STATUS = (SELECT USER_DEFINED_STATUS FROM OLTBS_CONTRACT WHERE CONTRACT_REF_NO = ?)))',
'Y~Y~Y~Y~Y~Y~',
'VARCHAR2~VARCHAR2~VARCHAR2~NUMBER~DATE~DATE~',
'I'
)
;

'CONTRACT_REF_NO~BRANCH~COUNTERPARTY~CUSTOMER_NAME1~VALUE_DATE~MATURITY_DATE~INT_PAID~TAX_YEAR~',
'LBL_CONTRREFNO~LBL_BRANCH~LBL_COUNTERPAERTY~LBL_CUSTOMERNAME~LBL_VALDATE~LBL_MATURITYDATE~LBL_INTPAID~LBL_TAX_YEAR~',
'CONTRACT_REF_NO~BRANCH~COUNTERPARTY~CUSTOMER_NAME1~VALUE_DATE~MATURITY_DATE~INT_PAID~TAX_YEAR~',
'SELECT A.CONTRACT_REF_NO, A.BRANCH, A.COUNTERPARTY, B.CUSTOMER_NAME1, A.VALUE_DATE, A.MATURITY_DATE, SUM(NVL(D.AMOUNT_SETTLED, 0)) - SUM(NVL(J.INTEREST_WAIVER_AMT, 0)) INT_PAID, H.TAX_YEAR FROM OLTBS_CONTRACT_MASTER A, OLTMS_CUSTOMER B, OLTBS_CONTRACT C, OLTBS_AMOUNT_PAID D, OLTMS_BRANCH E, OLTMS_PRODUCT_MASTER_LD F, OLTM_1098_PARAM H, OLTBS_CONTRACT_LIQ I, OLTBS_CONTRACT_LIQ_SUMMARY J WHERE C.BRANCH = GLOBAL.CURRENT_BRANCH AND A.CONTRACT_REF_NO = C.CONTRACT_REF_NO AND C.MODULE_CODE = ''OL'' AND C.PRODUCT_TYPE = ''L'' AND C.CONTRACT_STATUS IN (''A'', ''L'') AND A.VERSION_NO = C.LATEST_VERSION_NO AND B.CUSTOMER_NO = A.COUNTERPARTY AND H.BRANCH_CODE = C.BRANCH AND D.CONTRACT_REF_NO = A.CONTRACT_REF_NO AND D.COMPONENT = A.MAIN_COMP AND D.PAYMENT_STATUS <> ''V'' AND E.BRANCH_CODE = A.BRANCH AND TO_CHAR(D.PAID_DATE, ''YYYY'') = H.TAX_YEAR AND F.PRODUCT = C.PRODUCT_CODE AND NVL(F.ALLOW_1098_STMT, ''N'') = ''Y'' AND I.CONTRACT_REF_NO = D.CONTRACT_REF_NO AND I.COMPONENT = D.COMPONENT AND I.EVENT_SEQ_NO = D.EVENT_SEQ_NO AND J.CONTRACT_REF_NO = D.CONTRACT_REF_NO AND J.VALUE_DATE = D.PAID_DATE AND J.EVENT_SEQ_NO = D.EVENT_SEQ_NO AND C.CONTRACT_REF_NO NOT IN (SELECT G.CONTRACT_REF_NO FROM OLTBS_OVD_INTEREST G WHERE G.BRANCH_CODE = C.BRANCH AND G.TAX_YEAR = H.TAX_YEAR) GROUP BY A.CONTRACT_REF_NO, A.BRANCH, A.COUNTERPARTY, B.CUSTOMER_NAME1, A.VALUE_DATE, A.MATURITY_DATE, H.TAX_YEAR',
'Y~Y~Y~Y~Y~Y~Y~Y~',
'VARCHAR2~VARCHAR2~VARCHAR2~VARCHAR2~DATE~DATE~NUMBER~NUMBER~',
'')
;

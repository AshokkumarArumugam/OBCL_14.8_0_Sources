'SELECT A.CONTRACT_REF_NO, A.USER_REF_NO, A.PRODUCT_CODE, A.COUNTERPARTY, A.CONTRACT_CCY, A.LATEST_EVENT_SEQ_NO, B.PRINCIPAL_OUTSTANDING_BAL, D.PRODUCT_DESCRIPTION, F.SHORT_NAME FROM OLTBS_CONTRACT A, OLTBS_CONTRACT_BALANCE B, OLTBS_CONTRACT_MASTER G, OLTMS_PRODUCT D, OLTMS_CUSTOMER F WHERE A.CONTRACT_REF_NO = G.CONTRACT_REF_NO AND A.LATEST_VERSION_NO = G.VERSION_NO AND A.CONTRACT_STATUS = ''A'' AND A.PRODUCT_TYPE = ''L'' AND A.MODULE_CODE = ''OL'' AND A.CONTRACT_REF_NO = B.CONTRACT_REF_NO AND A.PRODUCT_CODE = D.PRODUCT_CODE AND A.COUNTERPARTY = F.CUSTOMER_NO /*AND G.ROLLOVER_ALLOWED = ''N''*/  AND (EXISTS (SELECT 1 FROM OLTM_PRODUCT_MASTER_LD P WHERE P.PRODUCT = A.PRODUCT_CODE AND P.PAYMENT_METHOD = ''B'' AND NOT EXISTS (SELECT 1 FROM LFTMS_PRODUCT_ICCF I WHERE I.PRODUCT = A.PRODUCT_CODE AND I.RATE_TYPE = ''S'' AND I.SHOWN_IN_CONTRACT_MAIN_SCREEN = ''Y'')) /*LSDiscount BA changes start*/ OR EXISTS (SELECT 1 FROM OLTM_PRODUCT_MASTER_LD r WHERE r.product = a.product_code AND r.payment_method IN (''T'', ''D'') AND (banker_acceptance = ''Y'' OR (EXISTS((SELECT 1 FROM OLTB_CONTRACT_PREFERENCE K WHERE K.CONTRACT_REF_NO = A.CONTRACT_REF_NO AND DISCOUNTED_SCH = ''Y'' AND VERSION_NO = (SELECT MAX(VERSION_NO) FROM OLTB_CONTRACT_PREFERENCE R WHERE R.CONTRACT_REF_NO = A.CONTRACT_REF_NO))))))) /*LSDiscount BA changes end*/ AND NOT EXISTS (SELECT 1 FROM OLTBS_CONTRACT_MERGE_MASTER M WHERE M.CONTRACT_REF_NO = A.CONTRACT_REF_NO AND M.AUTH_STAT = ''A'' AND M.MERGE_STATUS = ''U'') AND NOT EXISTS (SELECT 1 FROM OLTBS_CONTRACT_MERGE_MASTER M, OLTBS_CONTRACT_MERGE_DETAIL D WHERE M.AUTH_STAT = ''A'' AND M.MERGE_STATUS = ''U'' AND M.CONTRACT_REF_NO = D.CONTRACT_REF_NO AND D.CHILD_REF_NO = A.CONTRACT_REF_NO AND D.CHILD_STATUS = ''U'' AND M.MERGE_SERIAL_NO = D.MERGE_SERIAL_NO)',
'Y~Y~Y~Y~Y~N~Y~Y~Y~',
'VARCHAR2~VARCHAR2~VARCHAR2~VARCHAR2~VARCHAR2~NUMBER~NUMBER~VARCHAR2~VARCHAR2~',

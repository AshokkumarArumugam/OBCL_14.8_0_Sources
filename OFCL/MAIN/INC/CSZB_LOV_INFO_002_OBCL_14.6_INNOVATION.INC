 VALUES 
('OLDMNDSB',
'LOV_CONTREF',
'CONTRACT_REF_NO~DISBURSEMENT_MODE~VALDT~CURRENCY~AMOUNT_FINANCED~AMOUNT~',
'LBL_CONTRREFNO~LBL_DTYPE~LBL_FLD_VALUE_DATE~LBL_CONCCY~LBL_AMOUNT_FINANCED~LBL_AMTDSBR~',
'CONTRACT_REF_NO~DISBURSEMENT_MODE~VALDT~CURRENCY~AMOUNT_FINANCED~AMOUNT~',
'SELECT * FROM ((SELECT a.Contract_Ref_No, b.Disbursement_Mode,NULL Valdt, c.currency, c.amount_financed, c.amount FROM Oltbs_Contract a, Oltbs_Contract_Preference b, Oltbs_Contract_master C WHERE b.Contract_Ref_No = a.Contract_Ref_No AND a.Contract_Status = ''A'' AND a.Module_Code = ''OL'' AND a.Branch = Global.Current_Branch AND b.Disbursement_Mode = ''M'' AND a.Contract_Ref_No = c.Contract_Ref_No and a.LATEST_VERSION_NO = c.version_no AND ((a.auth_status = ''A'' /*AND A.CURR_EVENT_CODE <> ''DSBR''*/ ) OR ((a.auth_status <> ''A'' AND (select EVENT_CODE from oltb_contract_event_log ae where ae.contract_ref_no = a.Contract_Ref_No and ae.EVENT_CODE in ( ''DSBB'') and ae.AUTH_STATUS = ''U'') in(''DSBB'')) OR (a.auth_status <> ''A'' AND (select EVENT_CODE from oltb_contract_event_log ae where ae.contract_ref_no = a.Contract_Ref_No and ae.EVENT_CODE in ( ''DSBR'',''RVDB'') and ae.AUTH_STATUS = ''U'') in(''RVDB'', ''DSBR''))))) UNION (SELECT a.Contract_Ref_No, b.Disbursement_Mode, b.Due_Date AS Valdt, c.currency, c.amount_financed, c.amount FROM Oltbs_Contract a, Oltbs_Contract_Dsbr_Sch b, Oltbs_Contract_master C WHERE b.Contract_Ref_No = a.Contract_Ref_No AND a.Contract_Status = ''A'' AND a.Module_Code = ''OL'' AND a.Branch = Global.Current_Branch AND b.Disbursement_Mode = ''A'' AND b.Due_Date <= Global.Application_Date AND Nvl(b.Schedule_Applied, ''N'') = ''N'' AND Nvl(b.Sch_Reversed, ''N'') = ''N'' AND a.Contract_Ref_No = c.Contract_Ref_No and a.LATEST_VERSION_NO = c.version_no AND ((a.auth_status = ''A'' /*AND A.CURR_EVENT_CODE <> ''DSBR''*/ ) OR (a.auth_status <> ''A'' AND (select EVENT_CODE from oltb_contract_event_log ae where ae.contract_ref_no = a.Contract_Ref_No and ae.EVENT_CODE in ( ''DSBR'',''RVDB'') and ae.AUTH_STATUS = ''U'') in (''RVDB'',''DSBR'')))) UNION (SELECT a.Contract_Ref_No, ''I'' AS Disbursement_Mode, a.Value_Date Valdt, c.currency, c.amount_financed, c.amount FROM Oltbs_Contract_Init_Due a, Oltbs_Contract b, Oltbs_Contract_master C WHERE a.Value_'||
'Date <= Global.Application_Date AND b.Contract_Status = ''Y'' AND b.Branch = Global.Current_Branch AND b.Module_Code = ''OL'' AND b.Contract_Ref_No = a.Contract_Ref_No AND b.Contract_Ref_No = c.Contract_Ref_No and b.LATEST_VERSION_NO = c.version_no AND ((b.auth_status = ''A'' /*AND A.CURR_EVENT_CODE <> ''DSBR''*/ ) OR (b.auth_status <> ''A'' AND (select EVENT_CODE from oltb_contract_event_log ae where ae.contract_ref_no = b.Contract_Ref_No and ae.EVENT_CODE in (''RVDB'',''DSBR'') and ae.AUTH_STATUS = ''U'') in (''RVDB'',''DSBR''))))) ORDER BY Contract_Ref_No, Disbursement_Mode',
'Y~Y~Y~Y~Y~Y~',
'VARCHAR2~VARCHAR2~DATE~VARCHAR2~NUMBER~NUMBER~',
'I'
)
;

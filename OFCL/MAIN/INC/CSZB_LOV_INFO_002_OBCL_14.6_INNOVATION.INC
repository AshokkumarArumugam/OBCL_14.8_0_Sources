)
 VALUES 
('OLDCOCRV',
'LOV_CONTRACT_REF_NO',
'CONTRACT_REF_NO~CONTRACT_CCY~BRANCH~DEPARTMENT_CODE~PRODUCT_CODE~USER_REF_NO~CUSTOM_REF_NO~PRINCIPAL_OUTSTANDING_BAL~RESERVE_AMT~RECOVERY_AMT~WRITEOFF_AMT~FAS114_RESERVE_AMT~VALUE_DATE~CONTRA_GL_BAL~MATURITY_DATE~FACILITY_NAME~LINKED_TO_REF~PRODUCT_DESCRIPTION~',
'LBL_CONTRREFNO~LBL_OT_CONTCCY~LBL_ACBRN~N~N~N~N~N~N~N~N~N~N~N~N~N~N~N~',
'CONTRACT_REF_NO~CONTRACT_CCY~BRANCH~',
'SELECT A.CONTRACT_REF_NO,A.CONTRACT_CCY,A.BRANCH,A.DEPARTMENT_CODE,A.PRODUCT_CODE,A.USER_REF_NO,A.CUSTOM_REF_NO,B.PRINCIPAL_OUTSTANDING_BAL,NVL(B.RESERVE_AMT, 0) RESERVE_AMT,NVL(B.RECOVERY_AMT, 0) RECOVERY_AMT,NVL(B.WRITEOFF_AMT, 0) WRITEOFF_AMT,NVL(B.FAS114_RESERVE_AMT, 0) FAS114_RESERVE_AMT,C.VALUE_DATE,NVL(C.CONTRA_GL_BAL, 0) CONTRA_GL_BAL,C.MATURITY_DATE,C.FACILITY_NAME,NVL(E.LINKED_TO_REF, A.CONTRACT_REF_NO) LINKED_TO_REF,F.PRODUCT_DESCRIPTION FROM OLTBS_CONTRACT A,OLTBS_CONTRACT_BALANCE B,OLTBS_CONTRACT_MASTER C,OLTBS_CONTRACT_LINKAGES E,OLTMS_PRODUCT F WHERE C.VERSION_NO = (SELECT MAX(D.VERSION_NO) FROM OLTBS_CONTRACT_MASTER D WHERE D.CONTRACT_REF_NO = C.CONTRACT_REF_NO) AND A.CONTRACT_REF_NO = B.CONTRACT_REF_NO AND A.CONTRACT_REF_NO = C.CONTRACT_REF_NO AND A.PRODUCT_CODE = F.PRODUCT_CODE AND E.VERSION_NO = A.LATEST_VERSION_NO AND A.CONTRACT_REF_NO = E.CONTRACT_REF_NO AND (EXISTS(SELECT 1 FROM OLTBS_CONTRACT_PREFERENCE G WHERE G.CONTRACT_REF_NO = A.CONTRACT_REF_NO AND G.VERSION_NO = A.LATEST_VERSION_NO AND NVL(G.COC_VALUATION_REQD,''N'') = ''Y'') OR EXISTS(SELECT 1 FROM OLTBS_CONTRACT_PREFERENCE H,OLTBS_CONTRACT_LINKAGES I WHERE H.CONTRACT_REF_NO=I.LINKED_TO_REF AND H.VERSION_NO=(SELECT MAX(VERSION_NO) FROM OLTBS_CONTRACT_PREFERENCE J WHERE J.CONTRACT_REF_NO=H.CONTRACT_REF_NO) AND I.LINKED_TO_BRANCH = sypks_utils.get_branch(H.CONTRACT_REF_NO) AND I.CONTRACT_REF_NO=A.CONTRACT_REF_NO AND I.VERSION_NO =(SELECT MAX(VERSION_NO) FROM OLTBS_CONTRACT_LINKAGES K WHERE K.CONTRACT_REF_NO=I.CONTRACT_REF_NO) AND H.COC_VALUATION_REQD=''Y''))',
'Y~Y~Y~N~N~N~N~N~N~N~N~N~N~N~N~N~N~N~',
'VARCHAR2~VARCHAR2~VARCHAR2~VARCHAR2~VARCHAR2~VARCHAR2~VARCHAR2~NUMBER~NUMBER~NUMBER~NUMBER~NUMBER~DATE~NUMBER~DATE~VARCHAR2~VARCHAR2~VARCHAR2~',
'')
;

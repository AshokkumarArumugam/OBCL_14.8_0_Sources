'LOV_CONTRACT',
'CONTRACT_REF_NO~VALUE_DATE~MATURITY_DATE~COUNTERPARTY~LOAN_CURRENCY~LINKED_TO_CURRENCY~LINKED_TO_REF~COUNTER_PARTY_DESC~',
'LBL_DE_CONT_REFNO~LBL_DE_VALUE_DT~LBL_MATDAT~LBL_CONTERPAR~LBL_LOAN_CCY~LBL_LINKCCY~LBL_LINK_TO_REF~LBL_CNTRDESCR~',
'CONTRACT_REF_NO~VALUE_DATE~MATURITY_DATE~COUNTERPARTY~LOAN_CURRENCY~LINKED_TO_CURRENCY~LINKED_TO_REF~COUNTER_PARTY_DESC~',
'SELECT C.CONTRACT_REF_NO ,D.VALUE_DATE,D.MATURITY_DATE, D.COUNTERPARTY,E.LOAN_CURRENCY, E.LINKED_TO_CURRENCY, E.LINKED_TO_REF,NVL(F.CUSTOMER_NAME1, F.SHORT_NAME) COUNTER_PARTY_DESC FROM OLTBS_CONTRACT C JOIN OLTBS_CONTRACT_MASTER D ON C.CONTRACT_REF_NO = D.CONTRACT_REF_NO JOIN OLTBS_CONTRACT_LINKAGES E ON C.CONTRACT_REF_NO = E.CONTRACT_REF_NO JOIN OLTMS_CUSTOMER F ON F.CUSTOMER_NO = D.COUNTERPARTY WHERE C.MODULE_CODE = ''OL'' AND C.PRODUCT_TYPE = ''L'' AND C.CONTRACT_STATUS = ''A'' AND C.AUTH_STATUS = ''A'' AND EXISTS (SELECT 1 FROM OLTBS_CONTRACT_LINKAGES L WHERE L.CONTRACT_REF_NO = C.CONTRACT_REF_NO AND L.VERSION_NO = C.LATEST_VERSION_NO AND L.LINKAGE_VALID = ''Y'' AND L.LOAN_CURRENCY <> L.LINKED_TO_CURRENCY) AND D.VERSION_NO =(SELECT MAX(VERSION_NO) FROM OLTBS_CONTRACT_MASTER F WHERE F.CONTRACT_REF_NO =C.CONTRACT_REF_NO) AND E.VERSION_NO =(SELECT MAX(VERSION_NO) FROM OLTBS_CONTRACT_LINKAGES G WHERE G.CONTRACT_REF_NO =C.CONTRACT_REF_NO)',
'Y~Y~Y~Y~Y~Y~Y~Y~',
'VARCHAR2~DATE~DATE~VARCHAR2~VARCHAR2~VARCHAR2~VARCHAR2~VARCHAR2~',
'')
;
END;

BEGIN
INSERT INTO CSZB_LOV_INFO
(FUNCTION_ID,LOV_ID,FIELD_LIST,LABEL_LIST
,REDUCTION_LIST,LOV_QUERY,VISIBLE_LIST,DATATYPE_LIST,LOV_TYPE
)
 VALUES 
('COMMON',
'LOV_LBDDLINKAGEREFNO',
'LINKAGE_REF_NO~LINKAGE_TYPE~LIAB_NO~CUSTOMER_NO~BRANCH~CURRENCY~DESCRIPTION~EXPIRY_DATE~',
'LBL_LINKAGE_REF_NO~LBL_LINKAGETYPE~LBL_LIABILITYNO~LBL_CUSTOMER_NO~LBL_REVERSAL_AC_BRN~LBL_REQUESTED_CCY~LBL_ROLEDESCRIPTION~LBL_RKEYEXPDT~',
'LINKAGE_REF_NO~LINKAGE_TYPE~LIAB_NO~CUSTOMER_NO~BRANCH~CURRENCY~DESCRIPTION~EXPIRY_DATE~',
'SELECT * FROM ((SELECT l.Line_Code ||l.Line_Serial Linkage_Ref_No, ''F'' Linkage_Type, (SELECT Liab_No FROM Getm_Liab WHERE Id = l.Liab_Id) Liab_No, cust.CUSTOMER_NO Customer_No, l.Brn Branch, l.Line_Currency Currency, l.Description, l.Line_Expiry_Date Expiry_Date FROM Getm_Facility l, Getm_Liab_Cust cust WHERE l.Record_Stat = ''O'' AND l.Liab_Id = cust.Liab_Id AND l.Once_Auth = ''Y'' AND l.Availability_Flag = ''Y'' AND (l.Line_Currency = ? OR Nvl(l.Ccy_Restriction, ''N'') = ''N'') AND (l.Id, NVL(?, ?)) IN (SELECT b.Id, a.Customer_No FROM Getm_Facility_Cust_Rest a, Getm_Facility b WHERE b.Customer_Rest_Type = ''A'' AND a.Facility_Id = b.Id UNION (SELECT b.Id, c.Customer_No FROM Elvw_Customer c, Getm_Facility b WHERE b.Customer_Rest_Type = ''D'' AND c.Record_Stat = ''O'' AND c.Auth_Stat = ''A'' MINUS SELECT Facility_Id, Customer_No FROM Getm_Facility_Cust_Rest)) AND (l.Id, ?) IN (SELECT b.Id, a.Product_Code FROM Getm_Facility_Prod_Rest a, Getm_Facility b WHERE b.Product_Rest_Type = ''A'' AND a.Facility_Id = b.Id UNION (SELECT b.Id, c.Product_Code FROM Eltm_Product c, Getm_Facility b WHERE Product_Rest_Type = ''D'' AND c.Record_Stat = ''O'' AND c.Once_Auth = ''Y'' MINUS SELECT Facility_Id, Product_Code FROM Getm_Facility_Prod_Rest)) AND (l.Id, ?) IN (SELECT b.Id, a.Branch_Code FROM Getm_Facility_Brn_Rest a, Getm_Facility b WHERE b.Branch_Rest_Type = ''A'' AND a.Facility_Id = b.Id UNION (SELECT b.Id, a.Branch_Code FROM Eltm_Branch a, Getm_Facility b WHERE Branch_Rest_Type = ''D'' AND a.Record_Stat = ''O'' MINUS SELECT Facility_Id, Branch_Code FROM Getm_Facility_Brn_Rest)) AND (l.Id, ?) IN (SELECT b.Id, Ccy FROM Getm_Facility_Ccy_Rest a, Getm_Facility b WHERE b.Ccy_Rest_Type = ''A'' AND a.Facility_Id = b.Id UNION (SELECT b.Id, Ccy_Code Ccy FROM Cytm_Ccy_Defn_Master c, Getm_Facility b WHERE b.Ccy_Rest_Type = ''D'' AND c.Record_Stat = ''O'' AND c.Auth_Stat = ''A'' MINUS SELECT Facility_Id, Ccy FROM Getm_Facility_Ccy_Rest))) UNION (SELECT Pp.Pool_Code Linkage_Ref_No, ''P'||
''' Linkage_Type, (SELECT Liab_No FROM Getm_Liab WHERE Id = Pp.Liab_Id) Liab_No, cust.Customer_No Customer_No, Pp.BRANCH_CODE Branch, Pool_Ccy Currency, Pp.Pool_Description, NULL Expiry_Date FROM Getm_Pool Pp, Getm_Liab_Cust cust WHERE Pp.Record_Stat = ''O'' AND cust.Liab_Id = Pp.Liab_Id AND Pp.Once_Auth = ''Y'') UNION (SELECT c.Collateral_Code Linkage_Ref_No, ''T'' Linkage_Type, Li.Liab_No Liab_No, cust.Customer_No Customer_No, c.Branch_Code Branch, c.Collateral_Currency Currency, c.Collateral_Description, c.End_Date Expiry_Date FROM Getm_Liab Li, Getm_Collat c, GETM_LIAB_CUST cust WHERE Li.Id = c.Liab_Id AND cust.Liab_Id = c.Liab_Id AND c.Available = ''Y'' AND c.Record_Stat = ''O'' AND c.Once_Auth = ''Y'' UNION SELECT c.Collateral_Code Linkage_Ref_No, ''T'' Linkage_Type, (SELECT Liab_No FROM Getm_Liab WHERE Id = c.Liab_Id) Liab_No, cust.Customer_No Customer_No, c.Branch_Code Branch, c.Collateral_Currency Currency, c.Collateral_Description, c.End_Date Expiry_Date FROM Getm_Liab Li, Getm_Collat_Share s, Getm_Collat c, GETM_LIAB_CUST cust WHERE Li.Id = s.Liab_Id AND cust.Liab_Id = s.Liab_Id AND c.Id = s.Coll_Id AND c.Sharing_Req = ''Y'' AND c.Available = ''Y'' AND c.Record_Stat = ''O'' AND c.Once_Auth = ''Y'') UNION (SELECT x.liab_no Linkage_Ref_No, ''L'' Linkage_Type, x.liab_no Liab_No, y.customer_no Customer_No, x.liab_Branch Branch, x.liab_ccy Currency,x.liab_name description, NULL Expiry_Date FROM GETM_LIAB x , GETM_LIAB_CUST y WHERE x.id = y.liab_id AND x.record_stat = ''O'' AND y.record_stat = ''O'' AND x.Once_Auth = ''Y'' AND y.Once_Auth = ''Y'' ) ) WHERE Customer_No = Nvl(?, ?) AND Linkage_Type = ?',
'Y~Y~Y~Y~Y~Y~Y~Y~',
'VARCHAR2~CHAR~VARCHAR2~VARCHAR2~VARCHAR2~VARCHAR2~VARCHAR2~DATE~',
'I'
)
;
END;

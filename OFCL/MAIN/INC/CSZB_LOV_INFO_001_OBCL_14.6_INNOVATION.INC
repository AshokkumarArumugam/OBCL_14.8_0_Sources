,REDUCTION_LIST,LOV_QUERY,VISIBLE_LIST,DATATYPE_LIST,LOV_TYPE
)
 VALUES 
('LBCCONBW',
'LOV_CUST',
'CUSTOMER~GFCID~SHORTNAME~CUSTOMERNAME~',
'LBL_CCUSTOM~LBL_GFCID~LBL_DE_SHORT_NAME~LBL_CIF_SIG_NAME~',
'CUSTOMER~GFCID~SHORTNAME~CUSTOMERNAME~',
'select CUSTOMER, GFCID, SHORTNAME, CUSTOMERNAME from (select a.customer_no as "CUSTOMER", a.gfcid as "GFCID", a.short_name as "SHORTNAME", a.customer_name1 as "CUSTOMERNAME" from oltms_customer a where a.record_stat = ''O'' and not exists (select b.customer_no from LBTBS_CONTRACT_BORROWERS b where a.customer_no = b.customer_no and contract_ref_no = ? and version_no = ?) and ? = ''F'' UNION select a.customer_no as "CUSTOMER", b.gfcid as "GFCID", b.short_name as "SHORTNAME", b.customer_name1 as "CUSTOMERNAME" from LBTBS_CONTRACT_BORROWERS a, oltms_customer b where b.record_stat = ''O'' and a.customer_no = b.customer_no /*and a.contract_ref_no = :parameter.prm_facility_ref_no*/ and a.version_no = (select max(version_no) from LBTBS_CONTRACT_BORROWERS c where c.contract_ref_no = a.contract_ref_no) and not exists (select b.customer_no from LBTBS_CONTRACT_BORROWERS c where c.customer_no = b.customer_no and c.contract_ref_no = ? and c.version_no = ?) and ? <> ''F'') d where  exists (select 1 from olvws_product_customers o where o.product_code = ? and o.customer_no = d.CUSTOMER) order by CUSTOMER',
'Y~Y~Y~Y~',
'VARCHAR2~VARCHAR2~VARCHAR2~VARCHAR2~',
'I'
)
;
END;

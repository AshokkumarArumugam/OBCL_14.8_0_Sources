'SELECT * FROM (SELECT Tag_Name, Substr(Tag_Name, 2, (Length(Tag_Name) - 2)) Tag_Desc FROM Oltbs_Fft_Advice_Tags WHERE Module = ? AND Event = ? AND ? IS NOT NULL /*Event*/ UNION (SELECT * FROM (SELECT Tag_Name, Substr(Tag_Name, 2, (Length(Tag_Name) - 2)) Tag_Desc FROM Oltb_Module_Fft_Tags WHERE Module = ''AL'' UNION SELECT Tag_Name, Substr(Tag_Name, 2, (Length(Tag_Name) - 2)) Tag_Desc FROM Oltb_Module_Fft_Tags WHERE Module = ? AND /*Module Code */ Tag_Origin = ''S'' UNION SELECT Tag_Name, Substr(Tag_Name, 2, (Length(Tag_Name) - 2)) Tag_Desc FROM Oltb_Module_Fft_Tags WHERE Module = ? AND /*Module Code */ Tag_Origin = (Decode(?, /*MOD */ ''LB'', (Decode(?, /*PRD*/ ''C'', ''G'', ''P'')), ''LP'', ''S'', NULL)) /*:Parameter.Prm_Parent*/ UNION SELECT Tag_Name, Substr(Tag_Name, 2, (Length(Tag_Name) - 2)) Tag_Desc FROM Oltb_Module_Fft_Tags WHERE Module = ? AND /*Module Code */ Tag_Origin = (Decode(?, /*MOD */ ''LB'', (Decode(?, /*PRD*/ ''C'', NULL, ''G'')), NULL)) /*:Parameter.Prm_Gparent*/ UNION SELECT ''_US$'' || Field_Name || ''UE$_'', Field_Name Tag_Desc FROM Oltm_Product_Userdef_Fields WHERE Product_Code = sypks_utils.get_product(?) UNION SELECT ''_PUS$'' || Field_Name || ''PUE$_'', Field_Name Tag_Desc FROM Oltm_Product_Userdef_Fields WHERE Product_Code = sypks_utils.get_product(?) UNION SELECT ''_UDF$'' || Field_Name || ''UDF$_'', Field_Name Tag_Desc FROM Oltm_User_Fields WHERE Function_Id = (Decode(?, /*MOD */ ''LB'', (Decode(?, /*PRD*/ ''C'', NULL, ''OLDTRONL'')), NULL)) /*:Parameter.Prm_Drfun_Id*/ UNION SELECT ''_TUDF$'' || Field_Name || ''TUDF$_'', Field_Name Tag_Desc FROM Oltm_User_Fields WHERE Function_Id = (Decode(?, /*MOD */ ''LB'', ''OLDTRONL'', NULL)) /*:Parameter.Prm_Trfun_Id*/ UNION SELECT ''_FUDF$'' || Field_Name || ''FUDF$_'', Field_Name Tag_Desc FROM Oltm_User_Fields WHERE Function_Id = (Decode(?, /*MOD */ ''LB'', ''FCTRONL'', ''FC'', ''FCDTRONL'', NULL)) /*:Parameter.Prm_Fcfun_Id*/ UNION SELECT ''_UDFDESC$'' || Field_Name || ''UDFDESC$_'', Field_Name Ta'||
'g_Desc FROM Oltm_User_Fields WHERE Function_Id = (Decode(?, /*MOD */ ''LB'', (Decode(?, /*PRD*/ ''C'', NULL, ''OLDTRONL'')), NULL)) /*:Parameter.Prm_Drfun_Id*/ UNION SELECT ''_PUDFDESC$'' || Field_Name || ''PUDFDESC$_'', Field_Name Tag_Desc FROM Oltm_User_Fields WHERE Function_Id = (Decode(?, /*MOD */ ''LP'', ''LPDCOMNT'', NULL)) /*:Parameter.Prm_Ptfun_Id*/ UNION SELECT ''_FUDFDESC$'' || Field_Name || ''FUDFDESC$_'', Field_Name Tag_Desc FROM Oltm_User_Fields WHERE Function_Id = (Decode(?, /*MOD */ ''LB'', ''FCTRONL'', ''FC'', ''FCDTRONL'', NULL)) /*:Parameter.Prm_Fcfun_Id*/ UNION SELECT ''_TUDFDESC$'' || Field_Name || ''TUDFDESC$_'', Field_Name Tag_Desc FROM Oltm_User_Fields WHERE Function_Id = (Decode(?, /*MOD */ ''LB'', ''OLDTRONL'', NULL)) /*:Parameter.Prm_Trfun_Id*/ UNION SELECT b.Tag_Name, b.Tag_Desc FROM Oltms_Fft_Tags_Master a, Oltms_Fft_Tags_Detail b WHERE a.Message_Type = b.Message_Type AND a.Record_Stat = ''O'' AND a.Auth_Stat = ''A'' AND a.Message_Type = ''C'') WHERE ? IS NULL /*Event*/)) WHERE ? = ''C''',

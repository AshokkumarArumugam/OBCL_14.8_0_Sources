(FUNCTION_ID,LOV_ID,FIELD_LIST,LABEL_LIST
,REDUCTION_LIST,LOV_QUERY,VISIBLE_LIST,DATATYPE_LIST,LOV_TYPE
)
 VALUES 
('LBDTRONL',
'LOV_LBTRLINKAGEREFNO',
'DEFAULT_LIAB~LINKAGE_REF_NO~LINKAGE_TYPE~LIAB_NO~CUSTOMER_NO~BRANCH~CURRENCY~DESCRIPTION~EXPIRY_DATE~FACILITY_TYPE~',
'LBL_DEFAULT_LIAB~LBL_LINKAGE_REF_NO~LBL_LINKAGETYPE~LBL_LIABILITYNO~LBL_CUSTOMER_NO~LBL_REVERSAL_AC_BRN~LBL_REQUESTED_CCY~LBL_ROLEDESCRIPTION~LBL_RKEYEXPDT~LBL_FACILITYTYPE~',
'DEFAULT_LIAB~LINKAGE_REF_NO~LINKAGE_TYPE~LIAB_NO~CUSTOMER_NO~BRANCH~CURRENCY~DESCRIPTION~EXPIRY_DATE~FACILITY_TYPE~',
'SELECT * FROM ((SELECT ''A'' Default_Liab, l.Line_Code || l.Line_Serial Linkage_Ref_No, ''F'' Linkage_Type, (SELECT Liab_No FROM Sttm_Core_El_Liab WHERE Id = l.Liab_Id) Liab_No, Cust.Customer_No Customer_No, l.Brn Branch, l.Line_Currency Currency, l.Description, l.Line_Expiry_Date Expiry_Date, l.facility_type facility_type FROM Sttm_Core_El_Facility l, Sttm_Core_El_Liab_Cust Cust WHERE l.Record_Stat = ''O'' AND Cust.Record_Stat = ''O'' AND Cust.Auth_Stat = ''A'' AND l.Liab_Id = Cust.Liab_Id AND l.Once_Auth = ''Y'' AND l.Availability_Flag = ''Y'' AND Nvl(''CCY'', ?) = ''CCY'' AND Nvl(''LIABCIF'', ?) = ''LIABCIF'' AND Nvl(''CNPTY'', ?) = ''CNPTY'' AND Nvl(''PRDCD'', ?) = ''PRDCD'' AND Nvl(''BRN'', ?) = ''BRN'' AND Nvl(''CCY'', ?) = ''CCY'') UNION (SELECT ''A'' Default_Liab, Pp.Pool_Code Linkage_Ref_No, ''P'' Linkage_Type, (SELECT Liab_No FROM Sttm_Core_El_Liab WHERE Id = Pp.Liab_Id) Liab_No, Cust.Customer_No Customer_No, Pp.Branch_Code Branch, Pool_Ccy Currency, Pp.Pool_Description, NULL Expiry_Date, NULL facility_type FROM Sttm_Core_El_Pool Pp, Sttm_Core_El_Liab_Cust Cust WHERE Pp.Record_Stat = ''O'' AND Cust.Record_Stat = ''O'' AND Cust.Auth_Stat = ''A'' AND Cust.Liab_Id = Pp.Liab_Id AND Pp.Once_Auth = ''Y'') UNION (SELECT ''A'' Default_Liab, c.Collateral_Code Linkage_Ref_No, ''T'' Linkage_Type, (SELECT Liab_No FROM Sttm_Core_El_Liab WHERE Id = c.Liab_Id) Liab_No, Cust.Customer_No Customer_No, c.Branch_Code Branch, c.Collateral_Currency Currency, c.Collateral_Description, c.End_Date Expiry_Date, NULL facility_type FROM Sttm_Core_El_Collat c, Sttm_Core_El_Liab_Cust Cust WHERE Cust.Liab_Id = c.Liab_Id AND c.Available = ''Y'' AND c.Record_Stat = ''O'' AND Cust.Record_Stat = ''O'' AND Cust.Auth_Stat = ''A'' AND c.Once_Auth = ''Y'') UNION (SELECT ''A'' Default_Liab, c.Liab_No Linkage_Ref_No, ''L'' Linkage_Type, c.Liab_No, Cust.Customer_No Customer_No, c.Liab_Branch Branch, c.Liab_Ccy Currency, c.Liab_Name, NULL AS Expiry_Date, NULL facility_type FROM Sttm_Core_El_Liab c,'||
' Sttm_Core_El_Liab_Cust Cust WHERE Cust.Liab_Id = c.Id AND c.Record_Stat = ''O'' AND Cust.Record_Stat = ''O'' AND Cust.Auth_Stat = ''A'' AND c.Once_Auth = ''Y'')) WHERE Customer_No = Nvl(?, ?) AND Linkage_Type = ? AND ((''A'' = ?) OR (''S'' = ? AND Liab_No = ?) OR Default_Liab = ?)',
'Y~Y~Y~Y~Y~Y~Y~Y~Y~Y~',
'CHAR~VARCHAR2~CHAR~VARCHAR2~VARCHAR2~VARCHAR2~VARCHAR2~VARCHAR2~DATE~VARCHAR2~',
'E'
)
;
END;

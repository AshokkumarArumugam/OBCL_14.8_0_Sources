'with part_prod as(SELECT opmi.participant_product participant_product FROM oltms_product_master_ld opmi WHERE product = ?),instrquery as(SELECT oi.counterparty counterparty, oi.branch branch, oi.module module, oi.product_code product_code, oi.currency currency, oi.ssi_mnemonic ssi_mnemonic, oi.settlement_seq_no settlement_seq_no, oi.pay_account pay_account, oi.recv_account recv_account, oi.record_stat record_stat FROM oltm_instr oi WHERE oi.counterparty || oi.currency || oi.module || oi.product_code || oi.branch = decode(oi.counterparty, ?, ?, ''ALL'') || decode(oi.currency, ?, ?, ''*.*'') || decode(oi.module, ''LS'', ''LS'', ''AL'') || decode(oi.product_code,(SELECT * from part_prod),(SELECT * from part_prod), ''ALL'') || decode(oi.branch, global.current_branch, global.current_branch, ''ALL''))select counterparty,branch,module,product_code,currency,ssi_mnemonic,settlement_seq_no,pay_account,recv_account from (select * from instrquery a where a.counterparty = ? and a.record_stat = ''O'' union select * from instrquery a where a.currency = ? and a.record_stat = ''O'' union select * from instrquery a where a.module = ''LS'' and a.record_stat = ''O'' union select * from instrquery a where a.product_code = (SELECT * from part_prod) and a.record_stat = ''O'' union select * from instrquery a where a.branch = global.current_branch and a.record_stat = ''O'') vc WHERE decode(vc.counterparty, ?, 1, 0) || decode(vc.currency, ?, 1, 0) || decode(vc.module, ''LS'', 1, 0) || decode(vc.product_code,( SELECT * from part_prod), 1, 0) || decode(vc.branch, global.current_branch, 1, 0) = ( SELECT MAX(decode(vc.counterparty, ?, 1, 0) || decode(vc.currency, ?, 1, 0) || decode(vc.module, ''LS'', 1, 0) || decode(vc.product_code,( SELECT * from part_prod ), 1, 0) || decode(vc.branch, global.current_branch, 1, 0)) FROM ( select * from instrquery b where b.counterparty = ? union select * from instrquery b where b.currency = ? union select * from instrquery b where b.module = ''LS'' union select'||
' * from instrquery b where b.product_code = (select * from part_prod) union select * from instrquery b where b.branch = global.current_branch ) vc )',
'Y~Y~Y~Y~Y~Y~Y~Y~Y~',
'VARCHAR2~VARCHAR2~VARCHAR2~VARCHAR2~VARCHAR2~VARCHAR2~NUMBER~VARCHAR2~VARCHAR2~',

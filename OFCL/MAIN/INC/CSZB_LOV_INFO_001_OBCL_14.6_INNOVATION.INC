,REDUCTION_LIST,LOV_QUERY,VISIBLE_LIST,DATATYPE_LIST,LOV_TYPE
)
 VALUES 
('LBDMKATV',
'LOV_CONTRACT',
'CONTRACT_REF_NO~COUNTERPARTY~CCY~PRINCIPALOUTSTANDINGBAL~VALUEDATE~MATURITYDATE~BRANCH~CUSTNAME~AMOUNT~FACILITYNAME~ADDLTEXT~',
'LBL_CONREFNO~LBL_COUNTPRT~LBL_Ccy~LBL_PRINC_OUTSTAND~LBL_DE_VALUE_DATE~LBL_MATDAT~LBL_ACBRN~LBL_DE_CUSTNAME1~LBL_RKYCNTRCTAMT~LBL_FACILITY_NAME~LBL_CPTYCNFRMKS~',
'CONTRACT_REF_NO~COUNTERPARTY~CCY~PRINCIPALOUTSTANDINGBAL~VALUEDATE~MATURITYDATE~BRANCH~CUSTNAME~AMOUNT~FACILITYNAME~ADDLTEXT~',
'SELECT dd.contract_ref_no CONTRACT_REF_NO,dd.Counterparty COUNTERPARTY,dd.contract_ccy CCY,(SELECT Principal_Outstanding_Bal FROM Oltbs_Contract_Balance WHERE Contract_Ref_No = dd.contract_ref_no) PRINCIPALOUTSTANDINGBAL,dd.Book_date VALUEDATE, aa.maturity_date MATURITYDATE,dd.branch BRANCH, (SELECT CUSTOMER_NAME1 FROM oltms_customer WHERE CUSTOMER_NO = dd.Counterparty) CUSTNAME,aa.amount_financed AMOUNT,aa.facility_name FACILITYNAME, (SELECT ADDL_TEXT FROM OLTBS_ADDL_TEXT D WHERE D.REFERENCE_NO = bb.contract_ref_no  AND D.EVNT_SEQ_NO = bb.event_seq_no) ADDLTEXT FROM OLtbs_contract dd, Oltbs_contract_event_log bb,(select x.contract_ref_no,x.amount_financed, x.facility_name, x.maturity_date From oltbs_contract_master x where x.version_no = (select max(om.version_no) from Oltbs_contract_master om WHERE om.contract_ref_no = x.contract_ref_no) UNION select y.contract_ref_no,y.amount,y.facility_name,y.facility_end_date From Lbtbs_syndication_master y where y.version_no = (select max(lsm.version_no) from Lbtbs_syndication_master lsm WHERE lsm.contract_ref_no = y.contract_ref_no)) aa WHERE dd.contract_ref_no = bb.contract_ref_no AND bb.event_seq_no = dd.latest_event_seq_no AND dd.contract_ref_no = aa.contract_ref_no(+) AND dd.contract_ref_no IN (SELECT DISTINCT cc.contract_ref_no FROM OLtbs_contract cc WHERE cc.module_code IN (''FC'', ''LB'') AND cc.contract_status = ''L'' AND /*cc.auth_status = ''A''*/ (cc.auth_status = ''A'' or ( cc.auth_status = ''U'' AND EXISTS (SELECT 1 FROM oltb_contract_event_log ae WHERE ae.contract_ref_no = cc.contract_ref_no AND ae.event_code = ''MRKA'' AND ae.auth_status = ''U'' ORDER BY EVENT_SEQ_NO DESC FETCH FIRST 1 Rows ONLY ))) AND cc.contract_ref_no NOT IN (SELECT a.contract_ref_no contract_ref_no FROM OLtbs_contract_split_rollover a WHERE a.contract_ref_no = cc.contract_ref_no AND a.version_no = (SELECT MAX(version_no) FROM OLtbs_contract_split_rollover WHERE contract_ref_no = a.contract_ref_no) AND a.rollover_contract_ref_no IS NOT NULL '||
'UNION SELECT b.child_ref_no contract_ref_no FROM lBtb_contract_consol_master a, lBtb_contract_consol_detail b WHERE a.contract_ref_no = b.contract_ref_no AND b.child_ref_no = cc.contract_ref_no AND a.rollover_contract_ref_no IS NOT NULL AND /*a.auth_stat = ''A''*/ (a.auth_stat = ''A'' or (a.auth_stat = ''U'' AND EXISTS (SELECT 1 FROM oltb_contract_event_log ae WHERE ae.contract_ref_no = cc.contract_ref_no AND ae.event_code = ''MRKA'' AND ae.auth_status = ''U'' ORDER BY EVENT_SEQ_NO DESC FETCH FIRST 1 Rows ONLY ))) AND a.record_stat = ''O'' UNION SELECT a.contract_ref_no contract_ref_no FROM lBtb_contract_split_detail a WHERE a.contract_ref_no = cc.contract_ref_no AND a.split_ref_no IS NOT NULL UNION SELECT b.child_ref_no contract_ref_no FROM lBtbs_contract_merge_master a, lBtbs_contract_merge_detail b WHERE a.contract_ref_no = b.contract_ref_no AND b.child_ref_no = cc.contract_ref_no AND a.merge_status = ''P'' AND /*a.auth_stat = ''A''*/ (a.auth_stat = ''A'' or ( a.auth_stat = ''U'' AND EXISTS (SELECT 1 FROM oltb_contract_event_log ae WHERE ae.contract_ref_no = cc.contract_ref_no AND ae.event_code = ''MRKA'' AND ae.auth_status = ''U'' ORDER BY EVENT_SEQ_NO DESC FETCH FIRST 1 Rows ONLY ))) AND a.record_stat = ''O''))',
'Y~Y~Y~Y~Y~Y~Y~Y~Y~Y~Y~',
'VARCHAR2~VARCHAR2~VARCHAR2~NUMBER~DATE~DATE~VARCHAR2~VARCHAR2~NUMBER~VARCHAR2~VARCHAR2~',
'I'
)
;
END;

'LBL_ACCLS~LBL_CS1~LBL_OVRDRFTFACLTY~',
'ACCOUNT_CLASS~DESCRIPTION~OVERDRAFT_FACILITY~',
'SELECT ACCOUNT_CLASS, DESCRIPTION, OVERDRAFT_FACILITY FROM ((SELECT A.ACCOUNT_CLASS, B.DESCRIPTION, NVL(B.OVERDRAFT_FACILITY, ''N'') OVERDRAFT_FACILITY FROM STVWS_ACLASS_BRANCHES A, STTMS_ACCOUNT_CLASS B WHERE A.ACCOUNT_CLASS = B.ACCOUNT_CLASS AND B.AC_CLASS_TYPE IN ( ''U'' , ''S'' ) AND NVL(B.END_DATE, GLOBAL.APPLICATION_DATE + 1) >= GLOBAL.APPLICATION_DATE AND A.BRANCH_CODE = GLOBAL.CURRENT_BRANCH AND B.AUTH_STAT = ''A'') INTERSECT (SELECT A.ACCOUNT_CLASS, B.DESCRIPTION, NVL(B.OVERDRAFT_FACILITY, ''N'') OVERDRAFT_FACILITY FROM SMVW_USER_ACCCLASS A, STTMS_ACCOUNT_CLASS B WHERE A.ACCOUNT_CLASS = B.ACCOUNT_CLASS AND (( NVL(B.MUDARABAH_ACC_CLASS,''N'' ) = ''N'' AND ''RET''=?) OR ''CSA'' =? OR ((NVL(B.MUDARABAH_ACC_CLASS, ''N'') = ''Y'' AND ? IN (''IJA'',''IST'',''MUD'',''MUR'',''MUS'',''TAW'')))) AND B.AC_CLASS_TYPE IN ( ''U'' , ''S'') AND NVL(B.END_DATE, GLOBAL.APPLICATION_DATE + 1) >= GLOBAL.APPLICATION_DATE AND A.USER_ID = GLOBAL.USER_ID AND B.AUTH_STAT = ''A'')) WHERE ACCOUNT_CLASS NOT IN (SELECT DISTINCT ACCOUNT_CLASS FROM ORTM_ACCOUNT_CLASS_MAINT WHERE APPLICATION_CATEGORY IN (SELECT APPLICATION_CATEGORY FROM ORTM_CATEGORY_MAINT WHERE ? NOT IN (''IJA'',''IST'',''MUD'',''MUR'',''MUS'',''TAW'',''RET'') AND APPLICATION_CATEGORY <> ?))',

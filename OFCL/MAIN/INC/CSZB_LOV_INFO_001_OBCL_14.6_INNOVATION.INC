)
 VALUES 
('COMMON',
'LOV_EXT_LOAN_ACCOUNT',
'CONTRACT_REF_NO~CONTRACT_CCY~MODULE_CODE~OUTSTANDING_AMT~COUNTERPARTY',
'LBL_LOAN_ACC_NO~LBL_CCY~LBL_MODULE~LBL_OUTSTAMT~LBL_CNPRTY',
'CONTRACT_REF_NO~CONTRACT_CCY~MODULE_CODE~OUTSTANDING_AMT~COUNTERPARTY',
'SELECT A.CONTRACT_REF_NO,' || chr(10) || '       A.CONTRACT_CCY,' || chr(10) || '       A.MODULE_CODE,' || chr(10) || '       NVL(fn_pck_liqd_amt(A.CONTRACT_REF_NO,' || chr(10) || '                           (SELECT TODAY' || chr(10) || '                              FROM STTM_DATES' || chr(10) || '                             WHERE BRANCH_CODE = A.BRANCH' || chr(10) || '                             )),' || chr(10) || '           0) OUTSTANDING_AMT,' || chr(10) || '       A.COUNTERPARTY' || chr(10) || '  FROM OLTBS_CONTRACT A, OLTBS_CONTRACT_MASTER B' || chr(10) || 'WHERE B.CONTRACT_REF_NO = A.CONTRACT_REF_NO' || chr(10) || '   AND B.VERSION_NO = A.LATEST_VERSION_NO' || chr(10) || '   AND B.BRANCH = A.BRANCH' || chr(10) || '   AND A.COUNTERPARTY = ?' || chr(10) || '   AND A.AUTH_STATUS = ''A''' || chr(10) || '   AND B.CONTRACT_STATUS = ''A''' || chr(10) || '   AND B.PACKING_CREDIT = ''Y'' ',
'Y~Y~Y~Y~N~',
'VARCHAR2~VARCHAR2~VARCHAR2~NUMBER~VARCHAR2~',
'I'
)

'||
''', Field_Name Tag_Desc FROM Oltm_User_Fields WHERE Function_Id = (Decode(?, /*MOD */ ''LB'', (Decode(?, /*PRD*/ ''C'', NULL, ''OLDTRONL'')), NULL)) /*:Parameter.Prm_Drfun_Id*/ UNION SELECT ''_PUDFDESC$'' || Field_Name || ''PUDFDESC$_'', Field_Name Tag_Desc FROM Oltm_User_Fields WHERE Function_Id = (Decode(?, /*MOD */ ''LP'', ''LPDCOMNT'', NULL)) /*:Parameter.Prm_Ptfun_Id*/ UNION SELECT ''_FUDFDESC$'' || Field_Name || ''FUDFDESC$_'', Field_Name Tag_Desc FROM Oltm_User_Fields WHERE Function_Id = (Decode(?, /*MOD */ ''LB'', ''FCTRONL'', ''FC'', ''FCDTRONL'', NULL)) /*:Parameter.Prm_Fcfun_Id*/ UNION SELECT ''_TUDFDESC$'' || Field_Name || ''TUDFDESC$_'', Field_Name Tag_Desc FROM Oltm_User_Fields WHERE Function_Id = (Decode(?, /*MOD */ ''LB'', ''OLDTRONL'', NULL)) /*:Parameter.Prm_Trfun_Id*/ UNION SELECT b.Tag_Name, b.Tag_Desc FROM Oltms_Fft_Tags_Master a, Oltms_Fft_Tags_Detail b WHERE a.Message_Type = b.Message_Type AND a.Record_Stat = ''O'' AND a.Auth_Stat = ''A'' AND a.Message_Type = ''C'') WHERE ? IS NULL /*Event*/)) WHERE ? = ''C'') UNION (SELECT b.Tag_Name, b.Tag_Desc FROM Oltms_Fft_Tags_Master a, Oltms_Fft_Tags_Detail b WHERE a.Message_Type = b.Message_Type AND a.Record_Stat = ''O'' AND a.Auth_Stat = ''A'' AND a.Message_Type = ''P'' AND ? = ''P''))',

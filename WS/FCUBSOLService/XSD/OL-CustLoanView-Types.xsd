                     </xs:simpleType>
                  </xs:element>
                  <xs:element name="XREF1" minOccurs="0">
                     <xs:simpleType>
                        <xs:restriction base="xs:string">
                           <xs:maxLength value="16"/>
                        </xs:restriction>
                     </xs:simpleType>
                  </xs:element>
                  <xs:element name="MODULECODE" minOccurs="0">
                     <xs:simpleType>
                        <xs:restriction base="xs:string">

<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.5">
    <display-name>OFCLTLServiceWebApp</display-name>
    <env-entry> 
        <description>Property File Path</description>
        <env-entry-name>propertyPath</env-entry-name> 
        <env-entry-type>java.lang.String</env-entry-type>  
        <env-entry-value>{GatewayPropFilePath}</env-entry-value>
    </env-entry> 
    <ejb-local-ref>
      <ejb-ref-name>FCUBS_FCSRVEJB_LOCAL</ejb-ref-name>
      <ejb-ref-type>Session</ejb-ref-type>
      <local>com.ofss.fcubs.service.ejb.FCSrvRouterEJBLocal</local>
      <ejb-link>../../FCServiceEJB.jar#FCSrvRouterEJB</ejb-link>
    </ejb-local-ref>
    <ejb-local-ref>
      <ejb-ref-name>FCUBS_FCMAINPROCEJB_LOCAL</ejb-ref-name>
      <ejb-ref-type>Session</ejb-ref-type>
      <local>com.ofss.fcubs.service.ejb.FCMainProcEJBLocal</local>
      <ejb-link>../../FCServiceEJB.jar#FCMainProcEJB</ejb-link>
    </ejb-local-ref>
    <ejb-local-ref>
      <ejb-ref-name>FCUBS_GWMSGPROCEJB_LOCAL</ejb-ref-name>
      <ejb-ref-type>Session</ejb-ref-type>
      <local>com.ofss.fcubs.gw.msgrouter.GWMsgProcEJBLocal</local>
      <ejb-link>../../GWMsgProcEJB.jar#GWMsgProcEJB</ejb-link>
    </ejb-local-ref>
</web-app>